@{
    ViewData["Title"] = "Cadastro";
}

<div class="text-center"><h3>Cadastro de Usuários</h3></div>

<div>
    <form>
        <div class="form-group">
            <label for="NameInput">Nome</label>
            <input type="email" class="form-control" id="NameIput" aria-describedby="Nome">
        </div>
            
        <div class="form-group">
            <label for="EmailInput">Email</label>
            <input type="email" class="form-control" id="NameIput" aria-describedby="Email">
        </div>
        <div class="form-group">
            <label for="ContatoInput">Contato</label>
            <input type="contato" class="form-control" id="ContatoInput" aria-describedby="Contato">
        </div>
        <button type="submit" class="btn btn-primary">Aceitar</button>
        <a type="button" class="btn btn-secondary" asp-controller="Home" asp-action="Index">Cancelar</a>
    </form>

</div>
